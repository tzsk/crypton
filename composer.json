{
    "name": "tzsk/crypton",
    "description": "Laravel API Request Response Encryption",
    "keywords": [
        "tzsk",
        "crypton",
        "laravel request encrypt",
        "laravel response encrypt"
    ],
    "homepage": "https://github.com/tzsk/crypton",
    "license": "MIT",
    "authors": [
        {
            "name": "Kazi Ahmed",
            "email": "mailtokmahmed@gmail.com",
            "homepage": "https://github.com/tzsk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/console": "^7.0|^8.0",
        "illuminate/encryption": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "laravel/legacy-factories": "^1.0",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Tzsk\\Crypton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tzsk\\Crypton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tzsk\\Crypton\\CryptonServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
