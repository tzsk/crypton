{
    "name": "tzsk/crypton",
    "description": "Laravel API Request Response Encryption",
    "license": "MIT",
    "authors": [
        {
            "name": "Kazi Mainuddin Ahmed",
            "email": "mailtokmahmed@gmail.com",
            "homepage": "https://tzsk.github.io"
        }
    ],
    "homepage": "https://github.com/tzsk/crypton",
    "keywords": ["Laravel", "Crypton"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tzsk\\Crypton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tzsk\\Crypton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tzsk\\Crypto\\cryptonServiceProvider"
            ]
        }
    }
}
